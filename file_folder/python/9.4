name = raw_input("Enter file:")
if len(name) < 1 : name = "mbox-short.txt"
handle = open(name)

d=list()


for lines in handle:
    lines=lines.rstrip()
    if lines=='':continue
    words=lines.split()
    if words[0]!='From':continue
    d.append(words[1])
count=dict()
for word in d:
    count[word]=count.get(word,0)+1

bigcount=None
bigword=None

for w,c in count.items():
    if bigcount==None or c>bigcount:
        bigword=w
        bigcount=c
print bigword,bigcount 
